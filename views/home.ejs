  <style>
    /* Adjust logo size */
    .navbar-brand img {
      width: 40px; /* Increase the width */
      height: 40px; /* Increase the height */
    }

    /* Adjust image sections */
    #main-image-section {
      height: 100vh; /* Cover the whole viewport height */
      width: 100%; /* Cover the whole viewport width */
      overflow: hidden; /* Hide any overflow to prevent scrollbars */
    }

    /* Set maximum height for additional image */
    #main-image-section img {
      max-height: 100vh; /* Set maximum height to cover viewport height */
      width: 100%; /* Cover the whole viewport width */
      object-fit: cover; /* Maintain aspect ratio while covering the viewport */
    }

    /* Set background color to white */
    body {
      background-color: #fff; /* White background color */
      font-family: Arial, sans-serif; /* Change font to Arial */
      margin: 0; /* Remove body margin */
      padding: 0; /* Remove body padding */
    }

    /* Why Us section styles */
    #why-us {
      color: #000; /* Text color */
      padding: 100px 0; /* Add padding */
      text-align: center;
    }

    #why-us h2 {
      font-size: 3rem; /* Larger font size */
    }

    #why-us p {
      font-size: 1.5rem; /* Larger font size */
      margin-bottom: 40px; /* Add some space below the paragraph */
    }

    /* Image slider item */
    .item img {
      width: 100%; /* Ensure images fill their container */
      height: auto; /* Maintain aspect ratio */
      max-height: 70vh; /* Set maximum height for slider images */
    }
  </style>
